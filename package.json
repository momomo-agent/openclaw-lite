{
  "name": "paw",
  "version": "0.12.1",
  "description": "Portable local AI assistant with multi-agent chat",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "dist": "electron-builder --mac",
    "dist:notarize": "electron-builder --mac && bash scripts/notarize.sh",
    "dist:notarize": "electron-builder --mac && bash scripts/notarize.sh",
    "dbb": "node .ai/dbb/dbb-test.js"
  },
  "author": "momomo-agent",
  "license": "MIT",
  "build": {
    "appId": "com.momomo.paw",
    "productName": "Paw",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg"
    },
    "files": ["main.js", "preload.js", "renderer/**/*", "assets/**/*", "package.json"],
    "dmg": { "title": "Paw" }
  },
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^26.8.1"
  }
}
